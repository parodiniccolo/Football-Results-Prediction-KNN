    "# We have 244 clubs that do not have scores, most probably due to some missing player in the dataset.\n",
    "# Let's find those clubs\n",
    "teams_21[teams_21['Age GK'].isnull()]['Club'].unique()\n",
    "\n",
    "# Remove clubs with NA values\n",
    "teams_21 = teams_21.dropna()\n",
    "\n",
    "# Read in match results from 2019-2020 season from 10 different European league + Champions League\n",
    "uk = pd.read_excel(\"data/all-euro-data-2019-2020.xls\", sheet_name = 'E0')\n",
    "be = pd.read_excel(\"data/all-euro-data-2019-2020.xls\", sheet_name = 'B1')\n",
    "fr = pd.read_excel(\"data/all-euro-data-2019-2020.xls\", sheet_name = 'F1')\n",
    "de = pd.read_excel(\"data/all-euro-data-2019-2020.xls\", sheet_name = 'D1')\n",
    "it = pd.read_excel(\"data/all-euro-data-2019-2020.xls\", sheet_name = 'I1')\n",
    "es = pd.read_excel(\"data/all-euro-data-2019-2020.xls\", sheet_name = 'SP1')\n",
    "nt = pd.read_excel(\"data/all-euro-data-2019-2020.xls\", sheet_name = 'N1')\n",
    "pt = pd.read_excel(\"data/all-euro-data-2019-2020.xls\", sheet_name = 'P1')\n",
    "tr = pd.read_excel(\"data/all-euro-data-2019-2020.xls\", sheet_name = 'T1')\n",
    "gr = pd.read_excel(\"data/all-euro-data-2019-2020.xls\", sheet_name = 'G1')\n",
    "ru = pd.read_excel(\"data/RU.xls\")\n",
    "cl = pd.read_excel(\"data/cl-2019-2020.xls\")\n",
    "\n",
    "\n",
    "# There are some discrepancies between match results dataset and FIFA 20/21 dataset\n",
    "# We'll adjust the names in match results to those in FIFA datasets\n",
    "\n",
    "\n",
