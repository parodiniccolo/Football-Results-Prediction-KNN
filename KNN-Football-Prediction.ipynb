   "source": [
    "# Add Zenit St. Petersburg and Ferencváros that are missing on fifa with stats of nearly ranked teams\n",
    "missing_teams = pd.read_csv(\"data/missing_teams.csv\")\n",
    "teams_21=teams_21.append([missing_teams])"
   ]
