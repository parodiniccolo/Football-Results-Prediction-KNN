    "def result(home_team, away_team):\n",
    "\n",
    "  # Create DF with the two teams and their attributes\n",
    "  d = {'HomeTeam': [home_team], 'AwayTeam': [away_team]}\n",
    "  match = pd.DataFrame(d)\n",
    "  table_m_1=pd.merge(match, home_teams_21,'left', left_on='HomeTeam',right_on='Home Club')\n",
    "  match_res=pd.merge(table_m_1, away_teams_21,'left',left_on='AwayTeam',right_on='Away Club')\n",
    "  match_res=match_res.drop(['Home Club','Away Club'],axis=1)\n",
    "\n",
    "  # Predict result\n",
    "  Xcl_match = scaler.transform(match_res.iloc[:,2:])\n",
    "  pred_match=knn.predict(Xcl_match)\n",
    "\n",
    "  match_res['Results']=pred_match\n",
    "  return match_res[['HomeTeam','AwayTeam','Results']]"
   ]
  },
