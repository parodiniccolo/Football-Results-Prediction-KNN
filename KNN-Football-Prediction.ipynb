    "def league_prediction(all_teams):\n",
    "  fixt=[]\n",
    "\n",
    "  fixt = pd.DataFrame(columns=['HomeTeam','AwayTeam'])\n",
    "\n",
    "  for i in all_teams[0].unique():\n",
    "      for k in all_teams[0].unique():\n",
    "          if i!=k:\n",
    "              fixt = fixt.append({'HomeTeam':i,'AwayTeam':k},ignore_index=True)\n",
    "            \n",
    "  fixt = fixt[fixt['HomeTeam'].notna()]\n",
    "  fixt\n",
    "\n",
    "  fixt_temp = pd.merge(fixt,home_teams_21,'left',left_on='HomeTeam',right_on='Home Club')\n",
    "  fixt_final = pd.merge(fixt_temp,away_teams_21,'left',left_on='AwayTeam',right_on='Away Club')\n",
    "  fixt_final = fixt_final.drop(['HomeTeam','AwayTeam','Home Club','Away Club'],axis=1)\n",
    "\n",
    "  fixt_df = scaler.transform(fixt_final)\n",
    "  pred = knn.predict(fixt_df)\n",
    "\n",
    "  #Converting result in points\n",
    "  fixt['Results']=pred\n",
    "  fixt['Homepts']=0\n",
    "  fixt['Awaypts']=0\n",
    "  fixt['Homepts'][fixt['Results']==1]=3\n",
    "  fixt['Awaypts'][fixt['Results']==2]=3\n",
    "  fixt['Homepts'][fixt['Results']==0]=1\n",
    "  fixt['Awaypts'][fixt['Results']==0]=1\n",
    "\n",
    "  hpts=fixt.groupby(['HomeTeam']).sum()\n",
    "  hpts=hpts.drop(['Awaypts','Results'],axis=1)\n",
    "  apts=fixt.groupby(['AwayTeam']).sum()\n",
    "  apts=apts.drop(['Homepts','Results'],axis=1)\n",
    "\n",
    "  hpts.reset_index(inplace=True)\n",
    "  apts.reset_index(inplace=True)\n",
    "  pred = pd.concat([hpts,apts],axis=1)\n",
    "  pred['Total Points']=pred['Homepts']+pred['Awaypts']\n",
    "  pred=pred.drop(columns=['Homepts','Awaypts','AwayTeam'],axis=1)\n",
    "  pred=pred.iloc[:,~pred.columns.duplicated()]\n",
    "  pred=pred.groupby(['HomeTeam']).sum()\n",
    "  return pred.sort_values(['Total Points'],ascending=False)"
   ]
